package multiClientsFrames;

import java.io.*;
import java.net.Socket;

public class ClientData {

    public String id;
    public Socket socket;
    public ObjectOutputStream out;
    public ObjectInputStream in;

    public ClientData(String id, Socket socket, ObjectOutputStream out, ObjectInputStream in) {
        this.id = id;
        this.socket = socket;
        this.out = out;
        this.in = in;
    }

}
