    }// </editor-fold>//GEN-END:initComponents

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        PrintWriter gnu = null;
        try {
            gnu = new PrintWriter(new File("files\\plot\\a.data"));
        } catch (Exception ex) {
            ex.printStackTrace();
        }
        String s = jTextArea1.getText();
        String[] x = s.split("\n");
        for (int i = 0; i < x.length; i++) {
            gnu.println(x[i]);
            gnu.flush();
        }
        gnu.close();
        //----------------------------------------------------

    }//GEN-LAST:event_jButton1ActionPerformed

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        try {
            Runtime rt = Runtime.getRuntime();
            Process proc = rt.exec("C:\\Program Files (x86)\\gnuplot\\bin\\gnuplot.exe");
            OutputStream opStream = proc.getOutputStream();
            PrintWriter gp = new PrintWriter(new BufferedWriter(new OutputStreamWriter(opStream)));

            gp.println("load 'files\\plot\\gnuFile.txt'; pause mouse close;\n");
            gp.close();

        } catch (Exception ex) {
            ex.printStackTrace();
        }
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        PrintWriter mat = null;
        try {
            mat = new PrintWriter(new File("files\\plot\\gnuFile.txt"));
        } catch (FileNotFoundException ex) {
            ex.printStackTrace();
        }
        mat.println("set title ' " + jTextField1.getText() + " '");
        mat.flush();
        mat.println("set xlabel '" + jTextField6.getText() + "'");
        mat.flush();
        mat.println("set ylabel '" + jTextField7.getText() + "'");
        mat.flush();
        mat.println("set xr [" + jTextField2.getText() + ":" + jTextField3.getText() + "]");
        mat.flush();
        mat.println("set yr [" + jTextField4.getText() + ":" + jTextField5.getText() + "]");
        mat.flush();
        mat.println("plot 'files\\plot\\a.data' using 1:2 with linespoints title '" + jTextField8.getText() + "' lt rgb '" + jComboBox1.getSelectedItem() + "' lw " + jComboBox2.getSelectedItem() + " ");
        mat.flush();
        mat.close();
    }//GEN-LAST:event_jButton3ActionPerformed

