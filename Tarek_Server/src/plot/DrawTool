    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton6ActionPerformed
        PrintWriter mat = null;
        try {
            mat = new PrintWriter(new File("files\\plot\\gnuFile.txt"));
        } catch (FileNotFoundException ex) {
            ex.printStackTrace();
        }
        mat.println("set title ' " + jTextField9.getText() + " '");
        mat.flush();
        mat.println("set xlabel '" + jTextField12.getText() + "'");
        mat.flush();
        mat.println("set ylabel '" + jTextField15.getText() + "'");
        mat.flush();
        mat.println("set xr [" + jTextField10.getText() + ":" + jTextField11.getText() + "]");
        mat.flush();
        mat.println("set yr [" + jTextField13.getText() + ":" + jTextField14.getText() + "]");
        mat.flush();
        String s = jTextArea3.getText();
        String x[] = s.split("\n");
        mat.print("plot 'files\\plot\\a.data' using 1:2 with linespoints title '" + x[0] + "' lw " + jComboBox2.getSelectedItem() + " ");
        mat.flush();
        for (int i = 1; i < x.length; i++) {
            mat.print(",'' using 1:" + (i + 2) + " with linespoints title '" + x[i] + "' lw " + jComboBox2.getSelectedItem() + " ");
            mat.flush();
        }
        mat.close();
    }//GEN-LAST:event_jButton6ActionPerformed

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton7ActionPerformed
        JFileChooser fc = new JFileChooser();
        fc.showOpenDialog(this);

        try {
            // Open an input stream
            this.setTitle(fc.getSelectedFile().getName());
            Scanner r = new Scanner(fc.getSelectedFile());
            String k = "";
            while (r.hasNextLine()) {
                k += r.nextLine() + "\n";
            }
            jTextArea1.setText(k);
        } catch (Exception e) {

        }
    }//GEN-LAST:event_jButton7ActionPerformed

