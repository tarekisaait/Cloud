    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton9ActionPerformed
        PrintWriter data = null;
        try {
            data = new PrintWriter(new File("files\\plot\\bars.data"));
        } catch (FileNotFoundException ex) {
            ex.printStackTrace();
        }
        data.println("Title " + jTextArea4.getText());
        data.flush();
        String x[] = jTextArea5.getText().split("\n");

        for (int i = 0; i < x.length; i++) {
            data.println(x[i]);
            data.flush();
        }

        data.close();
        //----------------------
    }//GEN-LAST:event_jButton9ActionPerformed

    private void jButton10ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton10ActionPerformed
        PrintWriter gnu = null;
        try {
            gnu = new PrintWriter(new File("files\\plot\\gnuPlotBars.plt"));
        } catch (FileNotFoundException ex) {
            ex.printStackTrace();
        }
        String s = "set title \"" + jTextField16.getText() + "\"\n"
                + "set auto x\n"
                + "set yrange [" + jTextField17.getText() + ":" + jTextField18.getText() + "]\n"
                + "set style data histogram\n"
                + "set ylabel '" + jTextField19.getText() + "'\n"
                + "set style histogram cluster gap 1\n"
                + "set style fill solid border -1\n"
                + "set boxwidth 0.9\n"
                + "set xtic scale 0\n"
                + "plot 'files\\plot\\bars.data' using 2:xtic(1) ti col fs pattern 1";

        String titles[] = jTextArea4.getText().split("\t");
        for (int i = 0; i < titles.length; i++) {
            s += ",'' u " + (i + 3) + " ti col fs pattern " + (i + 5);
        }
        gnu.println(s);
        gnu.flush();
        gnu.close();
    }//GEN-LAST:event_jButton10ActionPerformed

